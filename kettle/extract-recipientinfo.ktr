    <returning_graph>N</returning_graph>
    <return_graph_field />
    <mappings>
      <mapping>
        <parameter>rid</parameter>
        <field>rid</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>mid</parameter>
        <field>mid</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>rtype</parameter>
        <field>rtype</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>rvalue</parameter>
        <field>rvalue</field>
        <type>String</type>
      </mapping>
    </mappings>
    <returns>
