  <step>
    <name>Extract Recipient Info</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>enron-db</connection>
    <sql>SELECT
  rid
, mid
, rtype
, rvalue
FROM recipientinfo
WHERE rtype = 'TO' OR rtype = 'CC'
</sql>
    <limit>0</limit>
    <lookup />
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
    <attributes />
    <cluster_schema />
    <remotesteps>
