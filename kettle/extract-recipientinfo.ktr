      <yloc>192</yloc>
      <draw>Y</draw>
    </GUI>
  </step>
  <step>
    <name>Load CC Recipients</name>
    <type>Neo4jCypherOutput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>neo4j</connection>
    <cypher>UNWIND $rows AS row
	MATCH (m:Message {mid:row.mid})
	MATCH (e:EmailAddress {email:row.rvalue})
	CREATE (m)-[cc:CC]->(e)
	RETURN id(cc) AS cc, m.id AS mid, e.email AS email</cypher>
    <batch_size>500</batch_size>
    <read_only>N</read_only>
    <retry>Y</retry>
    <cypher_from_field>N</cypher_from_field>
    <cypher_field />
    <unwind>Y</unwind>
    <unwind_map>rows</unwind_map>
    <returning_graph>N</returning_graph>
    <return_graph_field />
    <mappings>
      <mapping>
        <parameter>rid</parameter>
        <field>rid</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>mid</parameter>
        <field>mid</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>rtype</parameter>
        <field>rtype</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>rvalue</parameter>
        <field>rvalue</field>
        <type>String</type>
      </mapping>
    </mappings>
    <returns>
      <return>
        <name>cc</name>
        <type>Integer</type>
        <source_type>Integer</source_type>
      </return>
      <return>
        <name>mid</name>
        <type>Integer</type>
        <source_type>Integer</source_type>
      </return>
      <return>
        <name>email</name>
        <type>String</type>
        <source_type>String</source_type>
      </return>
    </returns>
    <attributes />
    <cluster_schema />
    <remotesteps>
      <input>
      </input>
      <output>
      </output>
    </remotesteps>
    <GUI>
      <xloc>688</xloc>
      <yloc>272</yloc>
      <draw>Y</draw>
    </GUI>
  </step>
  <step>
    <name>Load TO Recipients</name>
    <type>Neo4jCypherOutput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
